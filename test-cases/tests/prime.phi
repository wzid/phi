func is_prime(n: int): int {
    int i = 2;
    
    if (n < 2) {
        return 0;
    }
    
    while (i * i < n + 1) {
        if (n % i == 0) {
            return 0;
        }
        i++;
    }
    
    return 1;
}

func main() {
    int i = 1000;
    int count = 0;
    while (i < 100000) {
        if (is_prime(i++)) {
            count++;
        }
    }
    printf("%d numbers are prime between 1000 and 100,000\n", count);
}